      plugin="org.springframework.ide.eclipse">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseUrl">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.wst.xml_ui.feature" match="greaterOrEqual"/>
